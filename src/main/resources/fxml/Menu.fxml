<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="menuPane" minHeight="0.0" minWidth="16.0" prefHeight="300.0" prefWidth="296.0"
            stylesheets="@../css/Menu.css" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="yousync.ui.MenuController">
    <Label layoutX="9.0" layoutY="16.0" text="Client ID" textAlignment="CENTER"
           AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="35.0"/>
    <PasswordField fx:id="clientIdBox" layoutX="9.0" layoutY="33.0" promptText="Insert Client ID"
                   AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="35.0"/>
    <Label layoutX="9.0" layoutY="65.0" text="Client Secret" textAlignment="CENTER" AnchorPane.leftAnchor="9.0"/>
    <PasswordField fx:id="clientSecretBox" layoutX="9.0" layoutY="82.0" promptText="Insert Client Secret"
                   AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="35.0"/>
    <Label layoutX="9.0" layoutY="117.0" prefHeight="17.0" prefWidth="54.0" text="Playlist ID" textAlignment="CENTER"
           AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="35.0"/>
    <TextField fx:id="playlistIdBox" layoutX="9.0" layoutY="134.0" promptText="Insert Playlist ID"
               AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="35.0"/>
    <Button fx:id="downloadButton" layoutX="73.0" layoutY="170.0" mnemonicParsing="false"
            onAction="#onDownloadButtonClick" text="Download" AnchorPane.rightAnchor="35.0"/>
    <Button fx:id="menuShow" ellipsisString="" layoutX="271.0" mnemonicParsing="false"
            text="&lt;|" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
</AnchorPane>
